{
    "name": "our-edu/laravel-translation-client",
    "description": "Laravel client package for consuming centralized translation service",
    "type": "library",
    "version": "1.0.1",
    "keywords": [
        "laravel",
        "translation",
        "i18n",
        "localization",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Our Education",
            "email": "dev@ouredu.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/translation": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "OurEdu\\TranslationClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OurEdu\\TranslationClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OurEdu\\TranslationClient\\TranslationServiceProvider"
            ],
            "aliases": {
                "Translation": "OurEdu\\TranslationClient\\Facades\\Translation"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}